podcastData:defineTable({
        title:v.string(),
        topic:v.string(),
        script:v.string(),
        videoStyle:v.string(),
        caption: v.any(),
        voice:v.string(),
        images:v.optional(v.any()),
        audioUrl:v.optional(v.string()),
        captionJson:v.optional(v.any()),
        uid:v.id('users'),
        createdBy: v.string()
       
    })